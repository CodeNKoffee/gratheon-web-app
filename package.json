{
	"private": true,
	"name": "web-app",
	"version": "1.0.0",
	"license": "AGPL",
	"scripts": {
		"develop": "ENV_ID=dev npm run -s dev",
		"build-clean": "rm -rf public/*",
		"build": "preact build --src src --template src/index.html --no-prerender --dest build",
		"build-move": "mv build/* public",
		"serve": "sirv build --cors --single",
		"dev": "./node_modules/.bin/preact watch",
		"prettier": "prettier --write --ignore-unknown .",
		"test": "jest --config test/jest.config.ts --runInBand --color",
		"test:e2e:write": "./node_modules/.bin/cypress open"
	},
	"dependencies": {
		"@edorivai/react-smooth-dnd": "0.11.2",
		"@urql/exchange-multipart-fetch": "1.0.1",
		"babel-polyfill": "6.26.0",
		"date-fns": "2.29.3",
		"dexie": "3.2.2",
		"dexie-react-hooks": "1.1.1",
		"graphql": "16.6.0",
		"graphql-ws": "5.11.2",
		"hls.js": "1.4.10",
		"leaflet": "1.9.3",
		"lodash": "4.17.21",
		"md5": "2.3.0",
		"preact": "10.11.3",
		"preact-cli": "3.4.3",
		"preact-cli-svg-loader": "1.1.0",
		"preact-render-to-string": "^5.2.1",
		"preact-router": "^3.2.1",
		"react": "18.2.0",
		"react-color": "2.19.3",
		"react-countdown-circle-timer": "3.2.1",
		"react-dom": "18.2.0",
		"react-helmet": "6.1.0",
		"react-leaflet": "4.2.0",
		"react-router": "6.6.1",
		"react-router-dom": "6.6.1",
		"recharts": "2.2.0",
		"regenerator-runtime": "0.13.11",
		"urql": "3.0.3",
		"webpack-pwa-manifest": "4.3.0",
		"wonka": "6.1.2"
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.12",
		"@types/jest": "^27.4.1",
		"@types/leaflet": "^1.8.0",
		"@types/lodash": "4.14.191",
		"@types/react-color": "3.0.6",
		"@types/recharts": "1.8.24",
		"cypress": "9.5.4",
		"enzyme": "3.11.0",
		"enzyme-adapter-preact-pure": "^4.0.1",
		"expect": "27.5.1",
		"identity-obj-proxy": "3.0.0",
		"jest": "29.3.1",
		"less": "^4.1.3",
		"less-loader": "^7.3.0",
		"lint-staged": "12.3.8",
		"per-env": "1.0.2",
		"preact-cli": "^3.4.0",
		"prettier": "2.6.2",
		"serve": "13.0.2",
		"sirv-cli": "^2.0.2",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"typescript": "^4.5.2",
		"webpack-bundle-analyzer": "4.5.0"
	},
	"jest": {
		"preset": "jest-preset-preact",
		"setupFiles": [
			"<rootDir>/tests/__mocks__/browserMocks.ts",
			"<rootDir>/tests/__mocks__/setupTests.ts"
		]
	}
